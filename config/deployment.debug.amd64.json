{
  "modulesContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": "",
            "registryCredentials": {
              "secondregistry": {
                "username": "SecondRegistry",
                "password": "SfiiV7uW+UXPMLkQ4EtJlMNJDLlCoXi1",
                "address": "secondregistry.azurecr.io"
              }
            }
          }
        },
        "systemModules": {
          "edgeAgent": {
            "type": "docker",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-agent:1.0",
              "createOptions": "{\"Env\":[\"IOTEDGE_WORKLOADURI=true\"]}"
            },
            "env": {
              "IOTEDGE_WORKLOADURI": {
                "value": true
              }
            }
          },
          "edgeHub": {
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "env": {
              "IOTEDGE_WORKLOADURI": {
                "value": true
              }
            },
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-hub:1.0",
              "createOptions": "{\"Env\":[\"IOTEDGE_WORKLOADURI=true\"],\"HostConfig\":{\"PortBindings\":{\"5671/tcp\":[{\"HostPort\":\"5671\"}],\"8883/tcp\":[{\"HostPort\":\"8883\"}],\"443/tcp\":[{\"HostPort\":\"443\"}]}}}"
            }
          }
        },
        "modules": {
          "tempSensor": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-simulated-temperature-sensor:1.0",
              "createOptions": "{\"Env\":[\"IOTEDGE_WORKLOADURI=true\"]}"
            },
            "env": {
              "IOTEDGE_WORKLOADURI": {
                "value": true
              }
            }
          },
          "FirstModule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "secondregistry.azurecr.io/firstmodule:0.0.1-amd64.debug",
              "createOptions": "{\"Env\":[\"IOTEDGE_WORKLOADURI=true\"]}"
            },
            "env": {
              "IOTEDGE_WORKLOADURI": {
                "value": true
              }
            }
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "routes": {
          "FirstModuleToIoTHub": "FROM /messages/modules/FirstModule/outputs/* INTO $upstream",
          "sensorToFirstModule": "FROM /messages/modules/tempSensor/outputs/temperatureOutput INTO BrokeredEndpoint(\"/modules/FirstModule/inputs/input1\")"
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    }
  }
}